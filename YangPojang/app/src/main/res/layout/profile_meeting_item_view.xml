<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="190dp"
    android:layout_height="180dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:background="@drawable/border_rounded_coner_gray"
    android:elevation="40dp">


    <TextView
        android:id="@+id/MyReviewShopName"
        android:layout_width="155dp"
        android:layout_height="20dp"
        android:gravity="center"
        android:text="meeting name"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/MyReviewShopAdd"
        android:layout_width="171dp"
        android:layout_height="25dp"
        android:text="address"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MyReviewShopName"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="56dp"
        android:layout_height="10dp"
        android:gravity="center"
        android:text="모인 사람"
        android:textAlignment="center"
        android:textSize="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MyReviewShopAdd"
        app:layout_constraintVertical_bias="0.0" />

    <ScrollView
        android:id="@+id/meeingMemberScroll"
        android:layout_width="83dp"
        android:layout_height="101dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <LinearLayout
            android:id="@+id/meeingMemberlinear"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>

    </ScrollView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="카테고리 : "
        android:textAlignment="center"
        android:textSize="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toEndOf="@+id/meeingMemberScroll"
        app:layout_constraintTop_toBottomOf="@+id/MyReviewShopAdd"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/MyReviewCategory"
        android:layout_width="58dp"
        android:layout_height="13dp"
        android:gravity="center"
        android:minWidth="0dp"
        android:minHeight="0dp"
        android:text="shopCategory"
        android:textAlignment="center"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/MyReviewShopAdd"
        app:layout_constraintVertical_bias="0.091" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="시간 : "
        android:textAlignment="center"
        android:textSize="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toEndOf="@+id/meeingMemberScroll"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/meetingTime"
        android:layout_width="47dp"
        android:layout_height="12dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="time"
        android:textAlignment="center"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/MyReviewCategory"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="나이 : "
        android:textAlignment="center"
        android:textSize="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toEndOf="@+id/meeingMemberScroll"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.206" />

    <TextView
        android:id="@+id/meetingAgeMin"
        android:layout_width="21dp"
        android:layout_height="11dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="N"
        android:textAlignment="center"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/meetingTime"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/wave"
        android:layout_width="wrap_content"
        android:layout_height="11dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="~"
        android:textAlignment="center"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/meetingAgeMin"
        app:layout_constraintTop_toBottomOf="@+id/meetingTime"
        app:layout_constraintVertical_bias="0.014" />

    <TextView
        android:id="@+id/meetingAgeMax"
        android:layout_width="21dp"
        android:layout_height="11dp"
        android:gravity="center"
        android:text="N"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toEndOf="@+id/wave"
        app:layout_constraintTop_toBottomOf="@+id/meetingTime"
        app:layout_constraintVertical_bias="0.069" />

    <TextView
        android:id="@+id/meetingMin"
        android:layout_width="18dp"
        android:layout_height="11dp"
        android:gravity="center"
        android:text="N"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.423"
        app:layout_constraintStart_toEndOf="@+id/meeingMemberScroll"
        app:layout_constraintTop_toBottomOf="@+id/meetingAgeMin"
        app:layout_constraintVertical_bias="0.321" />

    <TextView
        android:id="@+id/division"
        android:layout_width="wrap_content"
        android:layout_height="11dp"
        android:gravity="center"
        android:text="/"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toEndOf="@+id/meetingMin"
        app:layout_constraintTop_toBottomOf="@+id/meetingAgeMin"
        app:layout_constraintVertical_bias="0.321" />

    <TextView
        android:id="@+id/meetingMax"
        android:layout_width="18dp"
        android:layout_height="11dp"
        android:gravity="center"
        android:text="N"
        android:textAlignment="center"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toEndOf="@+id/division"
        app:layout_constraintTop_toBottomOf="@+id/meetingAgeMin"
        app:layout_constraintVertical_bias="0.321" />


    <Button
        android:id="@+id/cancleJoin"
        android:layout_width="51dp"
        android:layout_height="27dp"
        android:backgroundTint="#FF9800"
        android:gravity="center"
        android:padding="0dp"
        android:text="참석취소"
        android:textAlignment="center"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toEndOf="@+id/meeingMemberScroll"
        app:layout_constraintTop_toBottomOf="@+id/meetingMin"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>